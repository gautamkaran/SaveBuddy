{
    "manifest_version": 3,
    "version": "1.0",
    "name": "SaveBuddy",
    "description": "A simple extension to save and manage your links.",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "/assets/icons/icon16.png",
            "48": "/assets/icons/icon48.png",
            "128": "/assets/icons/icon128.png"
        }
    },
    "icons": {
        "16": "/assets/icons/icon16.png",
        "48": "/assets/icons/icon48.png",
        "128": "/assets/icons/icon128.png"
    },
    "permissions": [
        "tabs"
    ]
}